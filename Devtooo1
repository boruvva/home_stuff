#!/bin/bash

# Check if the number of levels to go up is provided
if [ -z "$1" ]; then
  echo "Usage: up <number_of_levels>"
  exit 1
fi

# Get the current directory
current_dir=$(pwd)

# Loop to go up the specified number of levels
for ((i=0; i<$1; i++)); do
  current_dir=$(dirname "$current_dir")
done

# Change to the new directory
cd "$current_dir"

# Print the new directory
pwd
