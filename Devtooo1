#!/bin/bash
# Check if the number of levels to go up is provided
if [ -z "$1" ]; then
  echo "Usage: $0 <number_of_levels>"
  exit 1
fi

# Set the current directory
current_dir=$(pwd)

# Loop to go up the specified number of levels
for ((i=0; i<$1; i++)); do
  current_dir=$(dirname "$current_dir")
done

# Change to the new directory
cd "$current_dir" || { echo "Failed to change directory"; exit 1; }

# Print the new directory
echo "$current_dir"
