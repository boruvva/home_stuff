#!/bin/bash

# Check if the number of levels to go up is provided
if [ -z "$1" ]; then
  echo "Usage: $0 <number_of_levels>"
  exit 1
fi

# Check if the provided argument is a number
if ! [[ "$1" =~ ^[0-9]+$ ]]; then
  echo "Error: Argument is not a number. Please provide a valid number of levels."
  exit 1
fi

# Set the current directory
current_dir=$(pwd
